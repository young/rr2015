{
  "env": {
    "browser": true,
    "es6":true
  },

  "globals": {
    // GSAP globals start
    "TweenLite": true,
    "TweenMax": true,
    "TimelineMax": true,
    "TimelineLite": true,
    "Back": true,
    "Bounce": true,
    "Circ": true,
    "Cubic": true,
    "Ease": true,
    "Elastic": true,
    "Elastic": true,
    "Expo": true,
    "Linear": true,
    "Power0": true,
    "Power1": true,
    "Power2": true,
    "Power3": true,
    "Power4": true,
    "Quad": true,
    "RoughEase": true,
    "Strong": true,
    "SlowMo": true,
    "SteppedEase": true,
    // GSAP globals end
    "react": true,
    "Stripe":true,
    "process":true,
    "module":true,
    "jest":true,
    "require":true,
    "__DEV__":true,
    "__STRIPE_KEY__":true,
    "__STAGING__":true,
    "__PRODUCTION__":true,
    "__ENV__":true,
    "_":true,
    "global": true,
    "__dirname": true,
    "describe": true,
    "it": true,
    "__CLIENT__": true,
    "__SERVER__": true,
    "__DEVELOPMENT__": true
  },
  "ecmaFeatures": {
    "blockBindings": true,
    "jsx": true,
    "classes": true
  },
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "no-underscore-dangle": [0],
    "no-console":[0],
    "strict": [0],
    "global-strict":[0],
    "no-constant-condition":[1],
    "valid-jsdoc":[0],
    "no-var":[1],
    "no-throw-literal":[1],
    "indent": [2, 2],
    "no-unused-vars": [1],
    "consistent-return":[1],
    "semi":[1],
    "no-trailing-spaces":[1],
    "no-debugger":[1],
    "no-multi-spaces": [1],
    "key-spacing": [1],
    "no-extra-semi": [1],
    "comma-dangle":[1],
    "comma-spacing":[1],
    "react/jsx-quotes": 1,
    "react/jsx-no-undef": 1,
    "space-infix-ops": [0],
    "new-cap": [0],
    "eol-last": [1]
  }
}
