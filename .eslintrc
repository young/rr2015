{
  "env": {
    "browser": true,
    "es6":true
  },

  "globals": {
  },
  "ecmaFeatures": {
    "blockBindings": true,
    "jsx": true,
    "classes": true
  },
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "no-underscore-dangle": [0],
    "no-console":[0],
    "strict": [0],
    "global-strict":[0],
    "no-constant-condition":[1],
    "valid-jsdoc":[0],
    "no-var":[1],
    "no-throw-literal":[1],
    "indent": [2, 2],
    "no-unused-vars": [1],
    "consistent-return":[1],
    "semi":[1],
    "no-trailing-spaces":[1],
    "no-debugger":[1],
    "no-multi-spaces": [1],
    "key-spacing": [1],
    "no-extra-semi": [1],
    "comma-dangle":[1],
    "comma-spacing":[1],
    "react/jsx-quotes": 1,
    "react/jsx-no-undef": 1,
    "space-infix-ops": [0],
    "new-cap": [0],
    "eol-last": [1]
  }
}
